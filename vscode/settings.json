{
    "workbench.colorTheme": "Bearded Theme Arc EolStorm",
    "files.autoSave": "onFocusChange",
    "editor.tabCompletion": "on",
    "window.menuBarVisibility": "toggle",
    "editor.wordWrap": "bounded",
    "editor.wordWrapColumn": 120,
    "emmet.includeLanguages": {
        "javascript": "javascriptreact"
    },
    "emmet.showExpandedAbbreviation": "always",
    "files.insertFinalNewline": true,
    "editor.fontSize": 11,
    "extensions.ignoreRecommendations": false,
    // "go.useCodeSnippetsOnFunctionSuggest": true,
    // "go.useCodeSnippetsOnFunctionSuggestWithoutType": true,
    // "go.autocompleteUnimportedPackages": true,
    "zenMode.centerLayout": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "gitlens.views.repositories.files.layout": "tree",
    "tslint.configFile": "tslint.json",
    // "editor.matchBrackets": false,
    "bracket-pair-colorizer-2.highlightActiveScope": true,
    "bracket-pair-colorizer-2.colors": [
        "Gold",
        "Orchid",
        "LightSkyBlue",
        "Orange"
    ],
    "files.watcherExclude": {
        "**/dist/**": true
    },
    "markdownlint.config": {
        "MD007": {
            "indent": 1
        },
        "MD010": false,
        "MD012": false,
        "MD013": false,
        "MD024": false,
        "MD036": false
    },
    "markdown.extension.preview.autoShowPreviewToSide": false,
    "markdown-preview-enhanced.mermaidTheme": "default",
    "gitlens.mode.active": "zen",
    "terminal.integrated.fontFamily": "FiraCode Nerd Font",
    "markdown.extension.italic.indicator": "_",
    "markdown.extension.list.indentationSize": "inherit",
    "editor.formatOnSave": true,
    "flow.useNPMPackagedFlow": true,
    "javascript.validate.enable": false,
    "javascript.format.enable": false,
    // "eslint.autoFixOnSave": true,
    // "javascript.format"
    "prettier.bracketSameLine": true,
    "prettier.semi": false,
    "prettier.singleQuote": true,
    "prettier.trailingComma": "all",
    // "eslint.packageManager": "yarn",
    "zenMode.hideLineNumbers": false,
    "todo-tree.tree.showScanModeButton": false,
    // "editor.acceptSuggestionOnEnter": "off",
    "shellcheck.enableQuickFix": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "todo-tree.highlights.customHighlight": {
        "TODO": {
            // "icon": "check",
            "type": "text-and-comment",
            // "rulerColour": "#ffab00",
            "borderRadius": "1px solid",
            "foreground": "#fff",
            "background": "#ffab00"
        },
        "FIXME": {
            // "icon": "check",
            "type": "text-and-comment",
            // "rulerColour": "#ffab00",
            "borderRadius": "1px solid",
            "foreground": "#fff",
            "background": "#fe4f4f"
        },
    },
    "tabnine.experimentalAutoImports": true,
    "go.installDependenciesWhenBuilding": false,
    // "go.gocodeAutoBuild": true,
    "go.addTags": {
        "transform": "camelcase"
    },
    "gopls": {
        // "build.expandWorkspaceToModule": false,
        // "build.buildFlags": [
        //     "-tags"
        // ],
        // "build.allowModfileModifications": true,
        // "formatting.gofumpt": true,
        "formatting.local": "renault.com"
    },
    "go.toolsManagement.autoUpdate": true,
    "go.formatTool": "goimports",
    "go.lintTool": "golangci-lint",
    "go.lintFlags": [
        "--fast"
    ],
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "files.associations": {
        "*.rb.mu": "ruby"
    },
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.defaultProfile.linux": "zsh",
    "editor.insertSpaces": false,
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "XXX",
        "TODEL"
    ],
    "liveshare.guestApprovalRequired": true,
    "puppet.editorService.enable": false,
    "gitlab.ca": "",
    "gitlab.cert": "",
    // "prettier.configPath": ".eslint",
    "gitlab.remoteName": "origin",
    "gitlab.pipelineGitRemoteName": "origin",
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
    },
    "[javascriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[python]": {
        "editor.formatOnType": true
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "prettier.tabWidth": 4,
    "gitlens.graph.layout": "editor",
    "shellformat.flag": "-i 4 -ci",
    "presentation-mode.active": {
        "commands": [
            "workbench.action.closeSidebar"
        ],
        "editor.fontSize": 18,
        // "editor.matchBrackets": "never",
        // "editor.minimap.enabled": false,
        "editor.scrollbar.verticalScrollbarSize": 0,
        "workbench.activityBar.visible": false,
        "workbench.statusBar.visible": false,
        // "workbench.colorTheme": "GitHub Light High Contrast",
        "workbench.colorTheme": "Bearded Theme Milkshake Mint",
        // "workbench.colorCustomizations": {
        //     "[GitHub Dark Default]": {
        //         "activityBar.background": "#000",
        //         "editor.background": "#000",
        //         "editor.lineHighlightBackground": "#0000",
        //         "editor.lineHighlightBorder": "#0000",
        //         "editorWhitespace.foreground": "#59A5FC",
        //         "panel.background": "#000",
        //         "sideBar.background": "#000",
        //         "terminal.background": "#000"
        //     },
        //     "[GitHub Light Default]": {
        //         "activityBar.background": "#fff",
        //         "editor.background": "#fff",
        //         "editor.lineHighlightBackground": "#fff0",
        //         "editor.lineHighlightBorder": "#fff0",
        //         "editorWhitespace.foreground": "#1167D7",
        //         "panel.background": "#fff",
        //         "sideBar.background": "#fff",
        //         "terminal.background": "#fff"
        //     }
        // },
        // "window.zoomLevel": 2
    },
    "[shellscript]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "editor.fontFamily": "FiraCode Nerd Font",
    "editor.fontLigatures": false,
    "workbench.iconTheme": "bearded-icons",
    "workbench.startupEditor": "none",
    "liveshare.diagnosticLogging": true,
    "liveshare.diagnosticMode": true,
    "liveshare.launcherClient": "visualStudioCode",
    "go.editorContextMenuCommands": {
        "fillStruct": true
    },
    "java.format.settings.profile": "GoogleStyle",
    "java.format.settings.url": "https://raw.githubusercontent.com/google/styleguide/gh-pages/eclipse-java-google-style.xml",
}
